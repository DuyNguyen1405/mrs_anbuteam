<ul>
  <% if know_users %>
    <% know_users.each do |user| %>
      <%= render "pages/user", user: user %>
    <% end %>
    <div class="users-load-more">
      <%= link_to follow_users_path(type: "know_users") do %>
        <i class="fa fa-eye" aria-hidden="true"></i>
        <%= t("load_more") %>
      <% end %>
    </div>
  <% else %>
    <p class="no-content">
      <%= t "news_feed.no_content" %>
    </p>
  <% end %>
</ul>
